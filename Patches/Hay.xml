		<value>
			<harvestYield>11</harvestYield>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/RecipeDef[defName = "Make_Kibble"]/products/Kibble</xpath>
		<value>
			<Kibble>500</Kibble>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/RecipeDef[defName = "Make_Kibble"]/ingredients/li[2]/count</xpath>
		<value>
			<count>2</count>
