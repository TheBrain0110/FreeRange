      			<FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
			</statBases>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Kibble"]</xpath>
		<value>
			<stackLimit>1000</stackLimit>
		</value>
	</Operation>
